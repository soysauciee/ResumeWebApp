<html>
<head>
    <title>Skill</title>
</head>
<body>
<h1>Skill</h1>
<table border="1">
    <tr><th>Skill ID</th><th>Skill Name</th><th>Skill Description</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><%= result[i].skill_id%></td>
        <td><a href="/skill/?skill_id=<%= result[i].skill_id %>"><%= result[i].skill_name %></a></td>
        <td><%= result[i].description %></td>
    </tr>
    <% } %>
</table>
</body>

</html>