<html>
<head>
    <title>Account</title>
</head>
<body>
<h1>Account</h1>
<table border="1">
    <tr><th>Email</th> <th>First Name</th> <th>Last Name</th> </th><th>Action</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>

        <td><a href="/account/?account_id=<%= result[i].account_id %>"><%= result[i].email %></a></td>
        <td><%= result[i].first_name %></td>  <td><%= result[i].last_name %></td>
        <td><form action="/account/delete/">
                <input type="hidden" name="account_id" id="account_id" value="<%= result[i].account_id %>" />
                <input type="submit" value="Delete"/>
            </form>






    </tr>
    <% } %>
</table>
</body>

</html>